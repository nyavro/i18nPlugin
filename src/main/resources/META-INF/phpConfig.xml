<idea-plugin>
    <extensions defaultExtensionNs="com.intellij">
        <annotator language="PHP" implementationClass="com.eny.i18n.plugin.ide.annotator.PhpCompositeKeyAnnotator"/>
        <psi.referenceContributor language="PHP" implementation="com.eny.i18n.plugin.ide.references.code.PhpReferenceContributor"/>
        <completion.contributor language="PHP" implementationClass="com.eny.i18n.plugin.ide.completion.PhpCompletionContributor"/>
        <lang.foldingBuilder language="PHP" implementationClass="com.eny.i18n.plugin.ide.folding.PhpFoldingBuilder"/>
        <lang.documentationProvider language="PHP" implementationClass="com.eny.i18n.plugin.ide.hint.HintProvider"/>
        <projectService serviceImplementation="com.eny.i18n.plugin.ide.settings.PoSettings"/>
    </extensions>
    <extensions defaultExtensionNs="com.eny.i18n">
        <localizationSourceProvider implementation="com.eny.i18n.plugin.technology.po.PlainObjectLocalizationSources"/>
        <customSettings implementation="com.eny.i18n.plugin.vue.PoConfigurable"/>
        <technologyFactory implementation="com.eny.i18n.plugin.language.php.PhpLanguageFactory"/>
    </extensions>
</idea-plugin>