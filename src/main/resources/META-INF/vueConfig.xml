<idea-plugin>
    <extensions defaultExtensionNs="com.intellij">
        <annotator language="VueJS" implementationClass="com.eny.i18n.plugin.ide.annotator.VueCompositeKeyAnnotator"/>
        <psi.referenceContributor language="VueJS" implementation="com.eny.i18n.plugin.ide.references.code.VueReferenceContributor"/>
        <completion.contributor language="VueJS" implementationClass="com.eny.i18n.plugin.ide.completion.VueCompletionContributor"/>
        <lang.foldingBuilder language="HTML" implementationClass="com.eny.i18n.plugin.ide.folding.VueJsFoldingBuilder"/>
    </extensions>
    <extensions defaultExtensionNs="com.eny.i18n">
        <localizationSourceProvider implementation="com.eny.i18n.plugin.technology.vue.VueLocalizationSources"/>
        <localizationSourceProvider implementation="com.eny.i18n.plugin.technology.vue.VueSfcLocalizationSources"/>
    </extensions>
</idea-plugin>