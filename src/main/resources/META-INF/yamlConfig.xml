<idea-plugin>
    <extensions defaultExtensionNs="com.eny.i18n">
        <contentGenerator implementation="com.eny.i18n.plugin.localization.yaml.YamlContentGenerator"/>
        <customSettings implementation="com.eny.i18n.plugin.ide.settings.YamlConfigurable"/>
    </extensions>
    <extensions defaultExtensionNs="com.intellij">
        <psi.referenceContributor language="yaml" implementation="com.eny.i18n.plugin.ide.references.translation.YamlReferenceContributor"/>
        <projectService serviceImplementation="com.eny.i18n.plugin.ide.settings.YamlSettings"/>
    </extensions>
</idea-plugin>