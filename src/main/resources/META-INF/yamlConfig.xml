<idea-plugin>
    <extensions defaultExtensionNs="com.eny.i18n">
        <localizationFactory implementation="com.eny.i18n.plugin.yaml.YamlLocalizationFactory"/>
        <customSettings implementation="com.eny.i18n.plugin.yaml.YamlConfigurable"/>
    </extensions>
    <extensions defaultExtensionNs="com.intellij">
        <psi.referenceContributor language="yaml" implementation="com.eny.i18n.plugin.yaml.YamlReferenceContributor"/>
        <projectService serviceImplementation="com.eny.i18n.plugin.yaml.YamlSettings"/>
    </extensions>
</idea-plugin>